    <script>
        
       function fibonacci(n){
        var a=0;
        var b=1;
        for(var i=0;i<10;i++){
            document.write(" "+a);
            
            var c=(a+b);
            a=b;
            b=c;
        }
        
       }
       var n=prompt("ENTER lenght:");
        fibonacci(n);
    </script>
